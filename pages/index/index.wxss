/* components/sigin-input/sigin-input.wxss */

.page {
  box-sizing: border-box;
  position: relative;
  display: flex;
  width: 100%;
  background-color: #fff;
}

.page .canvas {
  box-sizing: border-box;
  flex: 1;
  border: 3px solid #ccc;
}

.page .btns {
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  flex-direction: column;
  width: 50px;
}

.page .btns .auto-width {
  flex: 1;
}



